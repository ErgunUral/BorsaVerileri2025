{
  "name": "trae-project",
  "private": true,
  "version": "0.0.2",
  "type": "module",
  "scripts": {
    "client:dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "check": "tsc --noEmit",
    "server:dev": "nodemon",
    "socket:dev": "tsx api/socket-server.ts",
    "dev": "concurrently \"npm run client:dev\" \"npm run server:dev\" \"npm run socket:dev\"",
    "test": "jest",
    "test:unit": "vitest run --reporter=verbose",
    "test:unit:watch": "vitest --reporter=verbose",
    "test:unit:ui": "vitest --ui",
    "test:integration": "jest --config jest.config.cjs --testPathPattern=integration",
    "test:backend": "jest --config jest.config.cjs",
    "test:backend:watch": "jest --config jest.config.cjs --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:unit && npm run test:backend && npm run test:e2e",
    "test:coverage": "vitest run --coverage && jest --config jest.config.cjs --coverage",
    "test:coverage:open": "npm run test:coverage && open coverage/lcov-report/index.html",
    "type-check": "tsc --noEmit",
    "start:prod": "concurrently \"npm run serve\" \"npm run server:prod\"",
    "serve": "npx serve -s dist -l 3000",
    "server:prod": "cd api && node server.js",
    "docker:build": "docker build -t borsa-analiz .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ],
    "src/**/*.{ts,tsx}": [
      "npm run type-check"
    ]
  },
  "dependencies": {
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-tabs": "^1.1.13",
    "@supabase/supabase-js": "^2.57.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/ioredis": "^4.28.10",
    "@types/jsonwebtoken": "^9.0.10",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.4.0",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "date-fns": "^3.0.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.8.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.511.0",
    "node-cron": "^3.0.3",
    "openai": "^5.22.0",
    "playwright": "^1.55.1",
    "puppeteer": "^21.6.0",
    "rate-limiter-flexible": "^5.0.3",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.28.1",
    "recharts": "^2.8.0",
    "redis": "^5.8.2",
    "serve": "^14.2.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.0.2",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.14.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.25.0",
    "@playwright/test": "^1.55.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/cheerio": "^0.22.35",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
