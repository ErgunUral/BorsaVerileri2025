    "concurrently": "^9.2.0",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-sonarjs": "^3.0.5",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.2.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.3",
    "prettier": "^3.6.2",
    "sonarjs": "^0.2.0",
    "supertest": "^6.3.4",
    "tailwindcss": "^3.4.17",
    "terser": "^5.44.0",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.1",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
